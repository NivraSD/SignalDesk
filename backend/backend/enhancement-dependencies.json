{
  "backend": {
    "description": "Add these dependencies to backend/package.json",
    "dependencies": {
      "chromadb": "^1.7.3",
      "@pinecone-database/pinecone": "^2.0.1",
      "weaviate-ts-client": "^2.1.1",
      "@xenova/transformers": "^2.6.0",
      "natural": "^6.10.0",
